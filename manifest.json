{
	"name": "Tab Revolution",
	"description": "Tab Revolution",
	"version": "0.1",
	"author": "Los Admins",
	
	"background": {
		"persistent": true,
		"scripts": ["back.js", "jquery-3.0.0.js"]
	},

	"content_scripts": {
		"js": ["main.js", "jquery-3.0.0.js"],
		"matches": [ "*://*/*" ]
	},

	"icons": {
		"48": "",
		"16": ""
	},

	"default_locale": "en",

	"permissions": [
		"http://*/*", "https://*/*", "tabs", "background"
	],
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"minimum_chrome_version": "40",
	"manifest_version": 2,
}
